{
    "name": "cms-boilerplate",
    "version": "1.0.0",
    "description": "Boilerplate template for CMS theme development",
    "scripts": {
        "dcu": "docker-compose up -d",
        "dcd": "docker-compose down",
        "predev:watch": "npm run format",
        "dev:watch": "gulp devWatch",
        "build": "gulp build",
        "dev:build": "gulp dev",
        "format": "prettier --write src/**/*.{js,jsx,scss}",
        "mkcert:generate": "mkcert -key-file certs/dev.key -cert-file certs/dev.crt domain.tld localhost"
    },
    "author": "Kenert Kulu <kenertkulu@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.10.4",
        "babel-loader": "^8.1.0",
        "browser-sync": "^2.26.12",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-concat": "^2.6.1",
        "gulp-sass": "^4.1.0",
        "gulp-sourcemaps": "^2.6.5",
        "node-sass": "^4.14.1",
        "prettier": "^2.1.2",
        "webpack": "^4.44.1",
        "webpack-stream": "^6.1.0"
    },
    "dependencies": {}
}
